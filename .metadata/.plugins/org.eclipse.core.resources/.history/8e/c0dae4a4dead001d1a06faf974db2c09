package sdbms;

public class Student {
	String id = "ABC";
	int age;
	String name;
	int marks;
	int count=101;
	public Student(String id, int age, String name, int marks) {
		this.id = id+count;
		this.age = age;
		this.name = name;
		this.marks = marks;
		count++;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getMarks() {
		return marks;
	}
	public void setMarks(int marks) {
		this.marks = marks;
	}
	
}

