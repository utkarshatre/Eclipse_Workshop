package com;
class Car extends Exception{
	String msg;
	public Car(String msg) {
		this.msg = msg;
	}
	public void output() throws Car{
		throw new Car("waste car");
	}
}
public class Practice{
	public static void main(String[] args) {
		Car c = new Car("a");
		try {
			c.output();
		}
		catch(Car e) {
			e.printStackTrace();
		}
	}
}