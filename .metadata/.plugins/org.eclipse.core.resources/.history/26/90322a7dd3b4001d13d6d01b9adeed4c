package com;
import java.util.*;
class Person1{
	String n1;
	Person1(String n1){
		this.n1=n1;
	}
	@Override
	public boolean equals(Object o) {
		Person1 p = (Person1)o;
		return this.n1.equals(p.n1);
	}
}
class Person2{
	String n2;
	Person2(String n2){
		this.n2=n2;
	}
	@Override
	public boolean equals(Object o) {
		Person2 p=(Person2)o;
		return this.n2.equals(p.n2);
	}
}
class Person{
	public static void main(String[] args) {
		Person1 p1 = new Person1("a");
		Person2 p2 = new Person2("a");
		System.out.println(p1.equals(p2));
	}
}



